{
  "rules": {
    "strict": 0,
    "no-ternary": "off",
    "comma-dangle": ["error", "always"],
    "no-console": "off",
    "no-useless-escape": "off"
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  }
}
